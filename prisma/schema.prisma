generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Review {
  email     String
  author    String   @db.VarChar(20)
  station   String   @db.VarChar(20)
  body      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  id        Int      @id @default(autoincrement())
}

model Station {
  stationNum Int    @id @default(autoincrement())
  realtime   String @db.VarChar(20)
  feeltime   String
  station    String
}

model StationLike {
  email      String
  stationNum Int
  id         Int    @id @default(autoincrement())
  station    String
}

model User {
  email    String
  name     String @db.VarChar(20)
  nickname String @db.VarChar(20)
  password String @db.VarChar(20)
  station  String @db.VarChar(30)
  id       Int    @id @default(autoincrement())
}
